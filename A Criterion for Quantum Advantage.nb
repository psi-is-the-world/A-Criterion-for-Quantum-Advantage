 RowBox[{
  RowBox[{"IsElementary", "[", "\[CapitalGamma]_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"output", " ", "=", " ", "\"\<IDK\>\""}], "}"}], ",", " ", "\n", 
    "\t", 
    RowBox[{
     RowBox[{"Do", "[", 
